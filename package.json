{
  "name": "moneymate",
  "version": "1.0.0",
  "private": true,
  "description": "MoneyMate - Income/Expense tracking with OCR slip upload",
  "scripts": {
    "docker:dev": "docker compose up -d --build",
    "docker:prod": "docker compose -f docker-compose.yml -f docker-compose.prod.yml up -d --build",
    "docker:down": "docker compose down --remove-orphans",
    "docker:logs": "docker compose logs -f",
    "docker:prune": "docker system prune -f",
    "local:dev": "pnpm run db:up && pnpm run db:migrate && pnpm -r --parallel run dev",
    "local:dev:client": "pnpm --filter client run dev",
    "local:dev:server": "pnpm --filter server run dev",
    "db:up": "docker compose up -d --wait postgres redis",
    "db:down": "docker compose down",
    "db:migrate": "pnpm --filter server run db:migrate",
    "db:studio": "pnpm --filter server run db:studio",
    "build": "pnpm -r run build",
    "lint": "pnpm -r run lint",
    "clean": "pnpm -r exec rm -rf node_modules dist .turbo"
  },
  "keywords": ["moneymate", "ocr", "expense-tracker", "microservices"],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
